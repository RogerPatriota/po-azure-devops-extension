{
    "manifestVersion": 1,
    "id": "po-extension",
    "name": "Product Owner Control",
    "publisher": "ntt-po",
    "version": "0.0.2",
    "description": "A Azure DevOps extension to control new projects and centralize all process",
    "public": false,
    "categories": ["Azure Boards"],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "demands": [
        "api-version/3.0"
    ],
    "scopes": [
        "vso.work"
    ],
    "icons": {
        "default": "images/download.jpg"
    },
    "baseUri": "https://localhost:8080",
    "contributions": [
         {
            "id": "po-control-hub",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-work-web.work-hub-group"
            ],
            "properties": {
                "name": "PO Control",
                "order": 30,
                "uri": "dist/index.html"
            }
        }
    ],
    "files": [
        {
            "path": "src/index.html",
            "addressable": true
        },
        {
            "path": "dist",
            "addressable": true
        },
        {
            "path": "images/download.jpg",
            "addressable": true
        }
    ]
}